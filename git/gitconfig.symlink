[user]
  name = Ben Scott
  email = ben@reload.me.uk
[core]
  excludesfile = ~/.gitignore_global
  autocrlf = input
	commitGraph = true
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[help]
  autocorrect = 1
[push]
  default = upstream
[merge]
  defaultToUpstream = true
[diff]
	algorithm = patience
[gc]
	writeCommitGraph = true
[protocol]
	version = 2
[pull]
	ff = only
[init]
	defaultBranch = main
